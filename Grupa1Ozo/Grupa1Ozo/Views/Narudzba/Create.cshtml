@model Grupa1Ozo.Models.Narudzba

@{
    ViewData["Title"] = "Create";
}

<h1>Dodaj</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form  asp-action="Create">
             <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                  <label asp-for="NarudzbaId" class="control-label"></label>
            <input asp-for="NarudzbaId" class="form-control" />
            <span asp-validation-for="NarudzbaId" class="text-danger"></span>
        </div>
            <div class="form-group">
                <label asp-for="DatumNarudzbe" class="control-label"></label>
                <input asp-for="DatumNarudzbe" class="form-control" />
                <span asp-validation-for="DatumNarudzbe" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="StatusNarudzbe" class="control-label"></label>
                <input asp-for="StatusNarudzbe" class="form-control" />
                <span asp-validation-for="StatusNarudzbe" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Dodaj" class="btn btn-primary" />
                    
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-controller="UslugaNarudzba" asp-action="Index">Natrag</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
